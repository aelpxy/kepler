{
  "name": "@aelpxy/kepler",
  "version": "0.0.0",
  "description": "A template for bootstrapping your backend within minutes.",
  "main": "./src/index.ts",
  "scripts": {
    "start": "export NODE_ENV=production && node ./build/main.ts",
    "build": "rm -rf build && pnpm db:generate && tsc",
    "dev": "export NODE_ENV=development && tsx watch ./src/main.ts | pino-pretty",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "email:dev": "email dev --dir=./src/emails",
    "clean": "rm -rf node_modules build",
    "format": "biome format --write **/*",
    "lint": "biome lint --write **/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aelpxy/kepler.git"
  },
  "keywords": [],
  "author": "aelpxy <aelpxy@velta.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aelpxy/kepler/issues"
  },
  "homepage": "https://github.com/aelpxy/kepler#readme",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/etag": "^6.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@react-email/components": "0.0.25",
    "@react-email/tailwind": "0.1.0",
    "argon2": "^0.41.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.2",
    "drizzle-zod": "^0.5.1",
    "fast-jwt": "^4.0.5",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^4.0.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "react-email": "3.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.7.7",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.11",
    "drizzle-kit": "^0.26.2",
    "pino-pretty": "^11.3.0",
    "react": "^18.3.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
